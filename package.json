{
    "name": "novel-nexus-nxrepo",
    "version": "1.0.0",
    "description": "Mono repo project for book recommendation and price",
    "include": [
        "src/**/*",
        "package.json"
    ],
    "scripts": {
        "lint:format": "pnpm nx run-many --target=lint --all --fix",
        "prettier:format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
        "frontend:start": "pnpm nx run nexus-frontend:serve",
        "frontend:test": "pnpm nx run nexus-frontend:test",
        "frontend:e2e": "pnpm nx e2e nexus-frontend-e2e",
        "backend:start": "pnpm nx run nexus-api:serve",
        "backend:test": "pnpm nx run nexus-api:test",
        "backend:e2e": "pnpm nx e2e nexus-api-e2e",
        "all:build": "pnpm nx run-many --target=build --all",
        "all:test": "pnpm nx run-many --target=test --all",
        "all:test:affected": "pnpm nx affected --target=test --base=main --head=HEAD"
    },
    "private": true,
    "author": "Bambang",
    "license": "ISC",
    "devDependencies": {
        "@fastify/static": "^6.10.2",
        "@nestjs/mapped-types": "^1.2.2",
        "@nestjs/platform-fastify": "^9.4.2",
        "@nestjs/schematics": "^9.1.0",
        "@nestjs/swagger": "^6.3.0",
        "@nestjs/testing": "^9.1.1",
        "@nestjs/typeorm": "^9.0.1",
        "@nx/cypress": "16.3.2",
        "@nx/eslint-plugin": "16.3.2",
        "@nx/jest": "16.3.2",
        "@nx/js": "16.3.2",
        "@nx/linter": "16.3.2",
        "@nx/nest": "^16.3.2",
        "@nx/next": "^16.3.2",
        "@nx/node": "16.3.2",
        "@nx/react": "16.3.2",
        "@nx/webpack": "16.3.2",
        "@testing-library/react": "14.0.0",
        "@types/jest": "^29.4.0",
        "@types/node": "18.14.2",
        "@types/react": "18.0.28",
        "@types/react-dom": "18.0.11",
        "@typescript-eslint/eslint-plugin": "^5.58.0",
        "@typescript-eslint/parser": "^5.58.0",
        "babel-jest": "^29.4.1",
        "cypress": "^12.14.0",
        "eslint": "~8.15.0",
        "eslint-config-next": "13.4.1",
        "eslint-config-prettier": "8.1.0",
        "eslint-plugin-cypress": "^2.10.3",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "jest": "^29.4.1",
        "jest-environment-jsdom": "^29.4.1",
        "jest-environment-node": "^29.4.1",
        "nx": "16.3.2",
        "nx-cloud": "latest",
        "pg": "^8.11.0",
        "postgres": "^3.3.5",
        "prettier": "^2.6.2",
        "ts-jest": "^29.1.0",
        "ts-node": "10.9.1",
        "typeorm": "^0.3.16",
        "typescript": "~5.0.2",
        "verdaccio": "^5.0.4",
        "vite": "^4.3.4"
    },
    "nx": {
        "targets": {}
    },
    "dependencies": {
        "@nestjs/common": "^9.1.1",
        "@nestjs/core": "^9.1.1",
        "@nestjs/platform-express": "^9.1.1",
        "axios": "^1.0.0",
        "next": "13.4.1",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "reflect-metadata": "^0.1.13",
        "rxjs": "^7.8.0",
        "tslib": "^2.3.0"
    }
}
